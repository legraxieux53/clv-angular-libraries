{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "module": "es2015",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es5",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "clv-angular-boot": [
        "./dist/clv-angular-boot"
      ],
      "clv-angular-boot/*": [
        "dist/clv-angular-boot/*"
      ],
      "clv-advanced-table": [
        "dist/clv-advanced-table"
      ],
      "clv-advanced-table/*": [
        "dist/clv-advanced-table/*"
      ],
      "clv-angular-pipes-utilities": [
        "dist/clv-angular-pipes-utilities"
      ],
      "clv-angular-pipes-utilities/*": [
        "dist/clv-angular-pipes-utilities/*"
      ],
      "clv-angular-directives-utilities": [
        "dist/clv-angular-directives-utilities"
      ],
      "clv-angular-directives-utilities/*": [
        "dist/clv-angular-directives-utilities/*"
      ],
      "clv-ts-utils": [
        "dist/clv-ts-utils"
      ],
      "clv-ts-utils/*": [
        "dist/clv-ts-utils/*"
      ]
    }
  }
}
